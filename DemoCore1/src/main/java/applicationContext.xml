<?xml version="1.0" encoding="UTF-8"?>
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
  
<bean id="stu" class="com.spring.DemoCore1.Student">  
<constructor-arg value="1"></constructor-arg>  
<constructor-arg value="arun"></constructor-arg>  
<constructor-arg ><ref bean="boo"/></constructor-arg> 
</bean>  
  
  
<bean id="boo" class="com.spring.DemoCore1.Book">  
<constructor-arg value="physics" ></constructor-arg>  
</bean>  
  
</beans>  